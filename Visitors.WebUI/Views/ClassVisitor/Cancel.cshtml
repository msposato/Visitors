@model Visitors.WebUI.Models.ClassVisitorViewModel

@{
  ViewBag.Title = "Cancel";
}
<h3>Are you sure you want to cancel this class visit?</h3>

@Html.Partial("_ClassVisitorDetails", Model)

@using (Html.BeginForm())
{
  @Html.AntiForgeryToken()
  <p>
    <input type="submit" value="Cancel This Visit" />
    |
        @Html.ActionLink("Return to Visit Details", "Details", new { id = Model.Id })
  </p>
}
